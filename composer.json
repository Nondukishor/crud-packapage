{
    "name": "nipu/curdpackage",
    "license": "MIT",
    "description": "Laravel CRUD Generator",
    "keywords": [
        "laravel",
        "crud"
    ],
    "authors": [
        {
            "name": "Nipu Chakraborty",
            "email": "pro.nipu@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8.0|^9.0|^10",
        "laravelcollective/html": "^5.3|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^5.7|^8.0|^9.0",
        "orchestra/testbench": "^3.3|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Nipu\\CurdPackage\\": "src/"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nipu\\CurdPackage\\CrudGeneratorServiceProvider"
            ]
        }
    }
}
